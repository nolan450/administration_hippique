{% extends 'base.html.twig' %}

{% block title %}Autres index{% endblock %}

{% block body %}
    <h1>Autres index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Experience</th>
                <th>Niveau</th>
                <th>Etat_general</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for autre in autres %}
            <tr>
                <td>{{ autre.id }}</td>
                <td>{{ autre.experience }}</td>
                <td>{{ autre.niveau }}</td>
                <td>{{ autre.etatGeneral }}</td>
                <td>
                    <a href="{{ path('app_autres_show', {'id': autre.id}) }}">show</a>
                    <a href="{{ path('app_autres_edit', {'id': autre.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="5">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('app_autres_new') }}">Create new</a>
{% endblock %}
